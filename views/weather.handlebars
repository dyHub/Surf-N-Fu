<!doctype html>

<html>
<head>

    <title>COGS120/CSE170 Skeleton</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- Bootstrap -->
    <link href="/css/bootstrap.min.css" rel="stylesheet">
    <link href="/css/bootstrap-theme.min.css" rel="stylesheet">
    <link href="/css/bootstrap-flex.css" rel="stylesheet">
    <link href="/css/weather.css" rel="stylesheet">
    <link href="/css/base.css" rel="stylesheet">
    <link href="/css/skeleton.css" rel="stylesheet">
    <script src="routes/index.js"></script>
    <script src="/js/weather.js"></script>
</head>
<body>
    {{> navigation}}
    <div class="container" style="padding-top: 50px">
        {{#each beaches}}
            <div class="row">
                <div class="col-md-12">
                    <div class="page-header">
                        <h1>{{this.name}}</h1>
                    </div>
                    {{#ifCond @index "<" 8}}
                        {{> weather_timeline weatherData=@root.weatherData tideTime=@root.tideTimesFirst}}
                    {{/ifCond}}
                    {{#ifCond @index ">=" 8}}
                        {{> weather_timeline weatherData=@root.weatherData tideTime=@root.tideTimesSecond}}
                    {{/ifCond}}
                </div>
            </div>
        {{/each}}
    </div>


    <script src="/js/jquery.js"></script>
    <script src="/js/bootstrap.min.js"></script>

    <script>
      (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
      (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
      m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
      })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

      ga('create', 'UA-87618309-1', 'auto');
      ga('send', 'pageview');

    </script>
    
</body>
</html>