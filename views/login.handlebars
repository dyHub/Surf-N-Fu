<!doctype html>

<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1">
	
	<!-- Bootstrap -->
	<link href="/css/bootstrap.min.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.min.css" rel="stylesheet">

	<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js">
	</script>       

	<script>
		function pullJSON(){
			
			$.ajax({
	    url: '/json/login.json',
	    dataType: 'text',
	    cache: false,
	    success: function(data) {
	        var obj = JSON.parse(data);

			document.getElementById("uname").value = obj.username;
			document.getElementById("password").value = obj.password;
			window.location = "/"
	    }
	});
	}
	</script>
</head>

<body>

	<h1><a href="/" style="text-decoration:none;"> Login </a></h1>

	<br/>

	<form>
	Username: <input id="uname" type="text" name="username"><br>

	Password: <input id="password" type="text" name="LastName"><br>

	<button type="button" onclick="pullJSON()"> Submit </button>

	</form>

	<script src="/js/jquery.js"></script>
	<script src="/js/bootstrap.min.js"></script>
</body>
</html>